{% extends 'base.html.twig' %}

{% block title %}UserProduct{% endblock %}

{% block body %}
    <h1>UserProduct</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ user_product.id }}</td>
            </tr>
            <tr>
                <th>Quantity</th>
                <td>{{ user_product.quantity }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ user_product.createdAt ? user_product.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('user_product_index') }}">back to list</a>

    <a href="{{ path('user_product_edit', {'id': user_product.id}) }}">edit</a>

    {{ include('user_product/_delete_form.html.twig') }}
{% endblock %}
